[user]
	name = Ken Williams
	email = kenahoo@gmail.com
[color]
	diff = auto
	status = auto
[core]
	filemode = false
	pager = less -S
[log]
	decorate = short
	date = relative
[merge]
	conflictstyle = diff3
[push]
	default = simple
[diff "xlsx"]
	binary = true
	textconv = xlsx2csv
[alias]
	lg = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset'
	ff = "!f() { git push . HEAD:$1; }; f"
	changelog = log --pretty=format:'* %s [%h]\n'
	lgnc = log --graph --pretty=format:'%h -%d %s (%cr) <%an>'
	pushitgood = push --force-with-lease
	ll = !git lg --all
	graphviz = "!f() { echo 'digraph git {' ; git log --pretty='format:  %h -> { %p }' \"$@\" | sed 's/[0-9a-f][0-9a-f]*/\"&\"/g' ; echo '}'; }; f"
	alias = "!f() { perl -ne 'print if /\\[alias\\]/ ... /^\\[/' ~/.gitconfig; }; f"
	merge-to = "!f() { git checkout $1 && git merge $2 && git checkout -; }; f"
